{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "background",
					"color": "#EFEFEF",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#1C1C1C",
					"name": "Foreground"
				},
				{
					"slug": "primary-50",
					"color": "#fdffe7",
					"name": "Primary 50"
				},
				{
					"slug": "primary-light",
					"color": "#f8ff41",
					"name": "Primary Light"
				},
				{
					"slug": "primary",
					"color": "#F7E700",
					"name": "Primary"
				},
				{
					"slug": "primary-dark",
					"color": "#d1b100",
					"name": "Primary Dark"
				},
				{
					"slug": "secondary-light",
					"color": "#7a41ff",
					"name": "Secondary Light"
				},
				{
					"slug": "secondary",
					"color": "#5e09f8",
					"name": "Secondary"
				},
				{
					"slug": "secondary-dark",
					"color": "#4208aa",
					"name": "Secondary"
				},
				{
					"slug": "tertiary-light",
					"color": "#ed36ff",
					"name": "Tertiary Light"
				},
				{
					"slug": "tertiary",
					"color": "#b40ebd",
					"name": "Tertiary"
				},
				{
					"slug": "tertiary-dark",
					"color": "#861388",
					"name": "Tertiary Dark"
				}
			],
			"gradients": [
				{
				"slug": "foreground-to-background",
				"gradient": "linear-gradient(90deg, var(--wp--preset--color--foreground), var(--wp--preset--color--background))",
				"name": "Foreground to background"
				}
			]
		},
		"layout": {
			"contentSize": "60rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"spacingScale": {
				"operator": "*",
				"increment": 2.0,
				"steps": 4,
				"mediumStep": 2,
				"unit": "rem"
			}
		}
	}
}
