{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "background",
					"color": "#EFEFEF",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#1C1C1C",
					"name": "Foreground"
				},
				{
					"slug": "primary-50",
					"color": "#fdffe7",
					"name": "Primary 50"
				},
				{
					"slug": "primary-300",
					"color": "#F8FF41",
					"name": "Primary 300"
				},
				{
					"slug": "primary-light",
					"color": "#f8ff41",
					"name": "Primary Light"
				},
				{
					"slug": "primary",
					"color": "#F7E700",
					"name": "Primary"
				},
				{
					"slug": "primary-dark",
					"color": "#d1b100",
					"name": "Primary Dark"
				},
				{
					"slug": "secondary-light",
					"color": "#7a41ff",
					"name": "Secondary Light"
				},
				{
					"slug": "secondary",
					"color": "#5e09f8",
					"name": "Secondary"
				},
				{
					"slug": "secondary-dark",
					"color": "#4208aa",
					"name": "Secondary"
				},
				{
					"slug": "tertiary-light",
					"color": "#ed36ff",
					"name": "Tertiary Light"
				},
				{
					"slug": "tertiary",
					"color": "#b40ebd",
					"name": "Tertiary"
				},
				{
					"slug": "tertiary-dark",
					"color": "#861388",
					"name": "Tertiary Dark"
				}
			],
			"gradients": [
				{
				"slug": "foreground-to-background",
				"gradient": "linear-gradient(90deg, rgba(28, 28, 28, 1) 0%, rgba(239, 239, 239, 1) 100%)",
				"name": "Foreground to background"
				}
			]
		},
		"layout": {
			"contentSize": "60rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"spacingScale": {
				"operator": "*",
				"increment": 2.0,
				"steps": 4,
				"mediumStep": 2,
				"unit": "rem"
			}
		},
		"blocks": {
			"core/button": {
				"appearanceTools": false,
				"color": {
					"background": false,
					"text": false
				},
				"typography": {
					"defaultFontSizes": false,
					"customFontSize": false,
					"fontStyle": false,
					"fontWeight": false,
					"fluid": false,
					"letterSpacing": false,
					"lineHeight": false,
					"textTransform": false,
					"fontSizes": [],
					"textDecoration": false
				},
				"spacing": {
					"margin": false,
					"padding": false
				},
				"shadow": {
					"defaultPresets": false,
					"presets": []
				},
				"border": {
					"radius": false
				}
			}
		}
	}
}
